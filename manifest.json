{
    "name": "Mobile view",
    "description": "A chrome mobile view extension for developers.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "warning.js"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "src/images/logo16x16.svg",
            "32": "src/images/logo32x32.svg",
            "48": "src/images/logo48x48.png",
            "128": "src/images/logo128x128.svg"
          }
      }
  }