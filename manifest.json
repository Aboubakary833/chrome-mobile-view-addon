{
    "name": "Mobile view",
    "description": "A mobile view extension for developers.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "src/js/app.js"
    },
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "src/images/get_started16.png",
            "32": "src/images/get_started32.png",
            "48": "src/images/get_started48.png",
            "128": "src/images/get_started128.png"
          }
      }
  }